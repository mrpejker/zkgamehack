#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../next@15.0.0-canary.148_react-dom@19.0.0-rc-4c2e457c7c-20240522_react@19.0.0-rc-4c2e457c7c-202_vlktecgcphddx3yv6qqxrhw42u/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../../../next@15.0.0-canary.148_react-dom@19.0.0-rc-4c2e457c7c-20240522_react@19.0.0-rc-4c2e457c7c-202_vlktecgcphddx3yv6qqxrhw42u/node_modules/next/dist/bin/next" "$@"
fi
