#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yakushkina/Mina/zknoidhack/node_modules/.pnpm/sort-package-json@2.10.1/node_modules/sort-package-json/node_modules:/Users/yakushkina/Mina/zknoidhack/node_modules/.pnpm/sort-package-json@2.10.1/node_modules:/Users/yakushkina/Mina/zknoidhack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yakushkina/Mina/zknoidhack/node_modules/.pnpm/sort-package-json@2.10.1/node_modules/sort-package-json/node_modules:/Users/yakushkina/Mina/zknoidhack/node_modules/.pnpm/sort-package-json@2.10.1/node_modules:/Users/yakushkina/Mina/zknoidhack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sort-package-json@2.10.1/node_modules/sort-package-json/cli.js" "$@"
else
  exec node  "$basedir/../../../../../sort-package-json@2.10.1/node_modules/sort-package-json/cli.js" "$@"
fi
